	2	;
	3	; Sample XM2 Assembler program to increment a number by 1
	4	; A. N. O'Nymous
	5	; 7 May 2019
	6	;
	7	; Data space
	8	org $80
	9	Number word #0
	10	;
	11	; Code
	12	org $1000
	13	Start
	14	;
	15	; R0 = Address of Number
	16	;
	17	movlz Number,R0
	18	;
	19	; R1 = Value stored in Number [R0]
	20	;
	21	ld R0,R1
	22	;
	23	; Increment R1
	24	;
	25	add #1,R1
	26	;
	27	; Number [R0] = R1
	28	;
	29	st R1,R0
	30	;
	31	; End of program
	32	; Specifying first executable location (Start)
	33	;
	34	end Start

